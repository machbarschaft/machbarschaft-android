        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.login.LoginMain"></activity>
        <activity android:name=".view.splash.splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.blabla.PhoneAuthstart" />
        <activity android:name=".view.blabla.VerifyPhoneActivity" />
        <activity android:name=".view.blabla.RegisterActivity" />
        <activity android:name=".view.blabla.LoginActivity" />
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>